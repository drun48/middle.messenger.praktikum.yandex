<div class="profile">
    <div class="profile__form">
        <div class="profile__form__title">
            {{> ProfileTitle title="Иван" photo="./src/assets/profile_photo.svg" chagePhoto=true }}
        </div>
        <div class="profile__form__info with-delimetr">
            {{#each (profileForm)}}
             {{> InputProfile }}
            {{/each}}
        </div>
        <div class="profile__form__changed with-delimetr">
            <a class="profile__form__changed__element">Изменить данные</a>
            <a class="profile__form__changed__element">Изменить пароль</a>
            <a class="profile__form__changed__element">Выйти</a>
        </div>
    </div>
    <div class="profile__back">
        <a class="profile__back__button">
            <img src="./src/assets/arrow-circle.svg">
        </a>
    </div>
</div>