<div class="baseModal container-form-modal">
    <div class="baseModal__title">
        <h2 class="{{#if errorTitle}}error{{/if}}">{{title}}</h2>
    </div>
    <div class="baseModal__content">
        {{> @partial-block}}
    </div>
    <footer>
        <button class="primary-button">{{labelButton}}</button>
        {{#if errorFooterText}}
            <p class="error">{{errorFooterText}}</p>
        {{/if}}
    </footer>
</div>